<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <record id="view_aut_ex_tree" model="ir.ui.view">
			<field name="name">aut.ex.tree</field>
			<field name="model">aut.ex</field>
			<field name="arch" type="xml">
				<tree string="Exoneraciones y Autorizaciones">
					<field name="code"/>
					<field name="name"/>
					<field name="active"/>
				</tree>
			</field>
		</record>

		<record id="action_aut_ex" model="ir.actions.act_window">
			<field name="name">Autorizaciones y Exoneraciones</field>
			<field name="res_model">aut.ex</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>

    </data>
</odoo>